TaskListActionsEnabled -> GmailQueryActionsEnabled;
TaskListWaitingSyncing -> SyncSyncingTaskLists;
TaskListWaitingSynced -> SyncTaskListsSynced;
GmailQueryWaitingThreadsFetched -> TaskListWaitingThreadsFetched;
GmailQueryWaitingMsgsFetched -> TaskListWaitingMsgsFetched;
TaskListWaitingEnabled -> GmailQueryWaitingEnabled;
TaskListSomedaySyncing -> SyncSyncingTaskLists;
TaskListSomedaySynced -> SyncTaskListsSynced;
GmailQuerySomedayThreadsFetched -> TaskListSomedayThreadsFetched;
GmailQuerySomedayMsgsFetched -> TaskListSomedayMsgsFetched;
TaskListSomedayEnabled -> GmailQuerySomedayEnabled
}